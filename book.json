{
    "plugins": [ "auto-summary","theme-api" ],
    "pluginsConfig": {
        "auto-summary": {
            "parts": { 
                "README.md": "Part I",
                "Users/**/*.md": "Part II",
                "Devops/**/*.md": "Part III"
            }      
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        }
    }
}
